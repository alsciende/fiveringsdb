<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}SovereignDB{% endblock %}</title>
    <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('images/favicons/apple-touch-icon.png') }}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('images/favicons/favicon-32x32.png') }}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('images/favicons/favicon-16x16.png') }}">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="{{ asset('styles/bootstrap.min.css') }}">
    <style type="text/css">
        @font-face {
            font-family: 'clans';
            src:  url({{ asset('fonts/clans.eot') }});
            src:  url({{ asset('fonts/clans.eot') ~ '#iefix' }}) format('embedded-opentype'),
            url({{ asset('fonts/clans.ttf') }}) format('truetype'),
            url({{ asset('fonts/clans.woff') }}) format('woff'),
            url({{ asset('fonts/clans.svg') ~ '#clans' }}) format('svg');
            font-weight: normal;
            font-style: normal;
            font-display: block;
        }
    </style>
    <link rel="stylesheet" href="{{ asset('styles/clans.css') }}">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Cinzel:wght@400..900&display=swap" rel="stylesheet">
    {% block stylesheets %}
    {% endblock %}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    {% block javascripts %}
        {% block importmap %}{{ importmap('app') }}{% endblock %}
    {% endblock %}
</head>
<body>
{% include 'navbar.html.twig' %}
{% include 'flash.html.twig' %}
{% block body %}{% endblock %}
{% include 'footer.html.twig' %}
</body>
</html>
